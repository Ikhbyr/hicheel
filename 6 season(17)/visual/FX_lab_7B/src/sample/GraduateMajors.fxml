<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="513.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GraduateMajors">
    <children>
        <TableView fx:id="dgvGraduateMajor" layoutX="21.0" layoutY="20.0" prefHeight="322.0" prefWidth="471.0">
            <columns>
                <TableColumn fx:id="colMajor" prefWidth="134.0" text="Major" />
                <TableColumn fx:id="colDean" prefWidth="110.0" text="Dean" />
                <TableColumn fx:id="colTitle" prefWidth="210.0" text="Title" />
            </columns>
        </TableView>
        <Button fx:id="btnClose" layoutX="431.0" layoutY="361.0" mnemonicParsing="false" onAction="#btnClose" prefHeight="25.0" prefWidth="62.0" text="Close..." />
        <Button layoutX="257.0" layoutY="361.0" mnemonicParsing="false" onAction="#btnNewGraduate" text="New Graduate Major..." />
      <Button fx:id="btnPrint" layoutX="21.0" layoutY="361.0" mnemonicParsing="false" text="Print Major" />
    </children>
</AnchorPane>
